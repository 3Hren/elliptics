{
    "version": 2,
    "paths": {
        "plugins": "/opt/elliptics/build/lib/cocaine",
        "runtime": "/opt/elliptics/build/tmp/cocaine/ipc",
        "spool": "/opt/elliptics/build/tmp/cocaine/spool"
    },  
    "services": {
        "logging": {
            "type": "logging",
            "args": {
		"port": 12501
            }   
        }
    },
    "storages": {
        "core": {
            "type": "elliptics",
            "args": {
                "nodes": {
                    "localhost": 1025
                },
                "groups": [2],
                "verbosity": 2
            }
        },  
        "cache": {
            "type": "files",
            "args": {
                "path": "/opt/elliptics/build/tmp/cocaine/cache"
            }   
        }   
    },  
    "loggers": {
        "core": {
            "type": "files",
            "args": {
		"path": "/tmp/cocaine.log",
		"verbosity": "error"
            }   
        },
        "slave": {
            "type": "remote",
            "args": {
	        "verbosity": "error"
	    }
        },
	"tools": {
		"type": "stdout"
	}
    },
    "port-mapper": {
        "range": [5100, 5200]
    }
}
